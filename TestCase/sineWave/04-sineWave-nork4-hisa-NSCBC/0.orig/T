FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type characteristicVelocityInletOutletTemperature;
        U (0 0 0);
        p 101325;
        T 300;
        value $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}


